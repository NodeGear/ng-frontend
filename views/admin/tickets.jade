extends layout

block content
	.row
		.col-lg-12.col-md-12.col-sm-12
			h1.page-header Support Tickets
	.row
		.col-lg-12.col-md-12.col-sm-12
			table.table
				thead
					tr
						th Subject
						th User
						th Created
						th &nbsp;
				tbody
					for ticket in tickets
						tr
							td= ticket.subject
							td= ticket.user.name
							td= ticket.created
							td