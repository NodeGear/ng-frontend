extends layout

block content
	.row
		.col-lg-12.col-md-12.col-sm-12
			h1.page-header Users
	.row
		.col-lg-12.col-md-12.col-sm-12
			table.table
				thead
					th Email
					th Name
					th &nbsp;
				tbody
					for user in users
						tr
							td= user.email
							td= user.name
							td
								.btn-group.pull-right
									a.btn.btn-xs.btn-default(href="/admin/user/"+user._id) Access